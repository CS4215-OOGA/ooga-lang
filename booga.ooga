type fmt struct{}

func (f *fmt) Println(x int) {
	print(x)
}

var fmt fmt = fmt{}

// USER PROGRAM
type Vertex struct {
	X int
}

func (v *Vertex) GetX() {
	for {
		fmt.Println(v.X)
	}
}

func main() {
	v := Vertex{1}
	w := Vertex{2}

	go v.GetX()
	go w.GetX()

	var i int
	for {
		i = 1
	}
}

main()
